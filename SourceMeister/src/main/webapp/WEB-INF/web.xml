<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" id="WebApp_ID" version="2.5">
  <display-name>SourceMeister</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.htm</welcome-file>
    <welcome-file>default.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
    <description></description>
    <display-name>ViewReplyWriteServlet</display-name>
    <servlet-name>ViewReplyWriteServlet</servlet-name>
    <servlet-class>com.meister.reply.web.ViewReplyWriteServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ViewReplyWriteServlet</servlet-name>
    <url-pattern>/reply/write</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>ViewReplyListServlet</display-name>
    <servlet-name>ViewReplyListServlet</servlet-name>
    <servlet-class>com.meister.reply.web.ViewReplyListServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ViewReplyListServlet</servlet-name>
    <url-pattern>/reply/list</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>DoDeleteActionServlet</display-name>
    <servlet-name>DoDeleteActionServlet</servlet-name>
    <servlet-class>com.meister.reply.web.DoDeleteActionServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>DoDeleteActionServlet</servlet-name>
    <url-pattern>/reply/delete</url-pattern>
  </servlet-mapping>
</web-app>